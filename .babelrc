{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-export-namespace-from",
    "@babel/plugin-transform-modules-commonjs"
  ],
  "ignore": [
    "src/**/*.test.js",
    "src/**/*.spec.js",
    "src/**/*.stories.js",
    "src/**/*.md",
    "src/**/*.mdx",
    "src/**/*.preview.js"
  ]
}
