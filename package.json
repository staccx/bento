{
  "name": "@staccx/bento",
  "version": "14.0.0",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage",
    "test:output": "jest --watch=false --json --outputFile=.jest-test-results.json",
    "build": "tsc",
    "start": "start-storybook -p 9009",
    "build-storybook": "tsc && build-storybook --output-dir public --quiet",
    "prepare": "npm run build",
    "lint": "eslint src/ --fix",
    "alpha": "npm version prerelease --preid=alpha && npm publish",
    "beta": "npm version prerelease --preid=beta && npm publish",
    "dirty-deploy-storybook": "npm run build-storybook && cp now.json public/ && cd public && now --prod",
    "chromatic": "CHROMATIC_APP_CODE=e5op6dsgv2n chromatic --exit-zero-on-changes",
    "semantic-release": "semantic-release",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/staccx/bento.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "module": "dist/index.js",
  "author": "",
  "license": "UNLICENSED",
  "homepage": "https://github.com/staccx/bento/blob/master/README.md",
  "sideEffects": false,
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@staccx/eslint-config": "^11.1.0",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-jest": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addon-storysource": "^6.4.15",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.20",
    "babel-loader": "^8.2.3",
    "husky": "7",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.2",
    "pinst": "^2.1.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-json-view": "^1.21.3",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "webpack": "^5.66.0"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "date-fns": "^2.28.0",
    "focus-trap": "^6.7.2",
    "fuse.js": "^6.5.3",
    "lodash.get": "^4.4.2",
    "react-flow-renderer": "^10.0.0-next.34",
    "react-query": "^3.34.11"
