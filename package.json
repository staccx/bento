{
  "name": "@staccx/bento",
  "version": "14.0.0",
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage --passWithNoTests --updateSnapshot",
    "build": "tsc",
    "start": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "prepare": "npm run build",
    "alpha": "npm version prerelease --preid=alpha && npm publish",
    "beta": "npm version prerelease --preid=beta && npm publish",
    "dirty-deploy-storybook": "npm run build-storybook && cp now.json public/ && cd public && now --prod",
    "chromatic": "CHROMATIC_APP_CODE=e5op6dsgv2n chromatic --exit-zero-on-changes",
    "semantic-release": "semantic-release",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/staccx/bento.git"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
